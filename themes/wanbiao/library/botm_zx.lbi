            </div></div>
          <div class="c2C mt10">
            <ul>
               <li>{insert name='article' num=5 article="$info.info_article"}</li>
            </ul>
          </div>
        </div>
        <div class="c3"> <div class="r1">
          <div class="tInfo1"><a href="{$info.240x320.link_url}" target="_blank">{$info.240x320.title_describe|escape:html}</a></div>
