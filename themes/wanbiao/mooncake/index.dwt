	<script type="text/javascript">
	function add_cart(goods_id)
	{
		var num = $('#num').val();
		var consignee = $('#consignee').val();
		var tel = $('#tel').val();
		var email = $('#email').val();
		var province = $('#province').val();
		var city = $('#city').val();
		var district = $('#district').val();
		var address = $('#address').val();
		var zipcode = $('#zipcode').val();
		var error = '';
		if(!(/^[0-9]{0,}$/.exec(num)))
			error = '购买数量不能为空';
		else if(consignee == '')
			error = '收货人不能为空';
		else if(!(/^[0-9]{0,}$/.exec(tel)))
			error = '请填写正确的联系电话';
		else if(!(/[\w\.\-]+@\w+\.(com.cn|cn|com|net)/.exec(email)))
			error = '请填写正确的邮箱地址';
		else if(province*1 < 1)
			error = '请选择省份';
		else if(city*1 < 1)
			error = '请选择市';
		else if(district*1 < 1)
			error = '请选择区域';
		else if(address=='')
			error = '详细地址不能为空';
		else if(!(/^[0-9]{0,}$/.exec(zipcode)))
			error = '请输入正确的邮政编码';
		if(error != '')
		{
			alert(error);
			return false;
		}
		var goods        = new Object();
		goods.quick    = 1;
		goods.goods_id = goods_id;
		goods.number   = num;
		goods.mooncake   = true;
		var error = '';
		$.post("/flow.php?step=add_to_cart", {goods:$.toJSON(goods)},function(data){
			if(data.error==0)
			{
				$('#theForm').submit();
			}
		}, "json");
	}
	// 弹窗1
  function showPop(selector){
    var target = $(selector),
      top = ($(window).height()-target.height())/2;
    target.css({
      'top':top +'px'
    }).show();
    $(".mengceng").height($(".lay").height());
    $(".mengceng").show();
    // 关闭弹窗
    target.find('.close_btn').click(function(){
      target.hide();
      $(".mengceng").hide();
    });
    target.find('.confirm_btn').click(function(){
      target.hide();
      $(".mengceng").hide();
    });
    target.find('.order_btn').click(function(){
      target.hide();
      $(".mengceng").hide();
    });
  }
function subtract()
{
	var sub_new_num = $('#num').val()*1-1*1;
	if(sub_new_num >= 1 )
	$('#num').attr('value',sub_new_num);
}
function addnum()
{
	var sub_new_num = $('#num').val()*1+1*1;
	$('#num').attr('value',sub_new_num);
}
if($('#province').val())
{
	getsite('province',2);
}	
function getsite(site,region_type)
{
	var parent_id = $('#'+site).val();
	$.get("/dateactivity.php", {act:'mooncake_site',parent_id:parent_id,region_type:region_type},function(data){
		var a="";
		if(data)
		{
			for(i=0;i<data.length;i++){
			a+='<option value='+data[i].region_id+'>'+data[i].region_name+'</option>';
			}
		}else{
			a+='<option value="">请选择</option>';
		}
		if(region_type == 2)
		{
			$('#city').html(a);
			getsite('city',3);
		}else if(region_type == 3)
		{
			$('#district').html(a);
		}
	}, "json");
}

	</script>
