popup = function(obj){
  //打开“覆盖层”
  open_area_overlay();
  //根据弹出框的宽高，设置其左右位置
  obj.css({
   'width':obj.width()+'px',
   'margin-left':'-'+obj.width()/2+'px'
  });

  //ie6
  if($.browser.msie && $.browser.version==6){
    obj.css({
     'top':$(document).scrollTop() + 150
    });
  }
  //显示
  obj.fadeIn();

  //关闭
  obj.find('.popup__close').add(area_overlay).click(function(e){
    obj.css('display','').removeAttr('style');
    close_area_overlay();
    
    e.preventDefault();
  })
}

